{
    "version": 2,
    "builds": [
      { "src": "*.html", "use": "@vercel/static" },
      { "src": "*.css", "use": "@vercel/static" },
      { "src": "*.js", "use": "@vercel/static" },
      { "src": "db.json", "use": "@vercel/json" }
    ],
    "routes": [
      { "src": "/", "dest": "/index.html" },
      { "src": "/api/userData", "dest": "/db.json" }
    ]
  }